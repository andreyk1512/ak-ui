{
  "name": "ak-ui",
  "version": "0.0.1",
  "type": "module",
  "description": "React UI kit library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "keywords": [
    "ui-kit",
    "component",
    "components",
    "design",
    "frontend",
    "react",
    "ui",
    "react-component"
  ],
  "author": "Andrii Krupa",
  "license": "MIT",
  "homepage": "https://github.com/andreyk1512/ak-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreyk1512/ak-ui.git"
  },
  "bugs": {
    "url": "https://github.com/andreyk1512/ak-ui/issues"
  },
  "scripts": {
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "tslint -p tsconfig.json",
    "test": "exit 0",
    "prepare": "pnpm build && (is-ci || husky install)",
    "pre-commit": "pnpm run lint",
    "pre-push": "pnpm run build && pnpm run test"
  },
  "devDependencies": {
    "@types/react": "^18.2.23",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "clsx": "^2.0.0"
  }
}
